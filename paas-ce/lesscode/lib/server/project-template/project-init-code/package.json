  "description": "",
  "scripts": {
    "clean": "rm -rf lib/client/dist",
    "clean-dll": "rm -rf lib/client/static/lib*",
    "build": "better-npm-run build",
    "local": "better-npm-run local",
    "online": "better-npm-run online",
    "dll": "better-npm-run dll",
    "dev": "better-npm-run dev",
    "heroku-prebuild": "./bin/pre-heroku"
  },
  "betterScripts": {
    "dll": {
      "command": "npm run clean-dll && node lib/client/build/build-dll.js"
    },
    "build": {
      "command": "npm run clean && npm run dll && node lib/client/build/build.js",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": ""
      }
    },
    "dev": {
      "command": "nodemon --config nodemon.json",
      "env": {
        "NODE_ENV": "development",
        "APP_CODE": ""
      }
    },
    "local": {
      "command": "npm run build && node lib/server/app.browser.js",
      "env": {
        "NODE_ENV": "local",
        "APP_CODE": ""
      }
    },
    "online": {
      "command": "node lib/server/app.browser.js",
      "env": {
        "NODE_ENV": "production",
        "APP_CODE": ""
      }
    }
  },
